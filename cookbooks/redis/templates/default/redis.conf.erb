daemonize yes
pidfile /var/run/redis.pid
port 6379
bind 0.0.0.0
timeout 300
loglevel notice
databases 16

save 900 1
save 300 10
save 60 10000

rdbcompression yes
dbfilename dump.rdb

dir /var/lib/redis

maxmemory 314572800

# advanced defaults from stock config
appendonly no
appendfsync everysec

activerehashing yes

